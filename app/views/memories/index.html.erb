<%= render 'shared/dashboard' %>

<div id="sidebar">
  <% @stamps.split.each do |stamp|%>
    <%= image_tag "stamp-rouge.svg"%>
  <% end%>
</div>

<div id="page-content">
  <div class="container">
    <div class="card-columns">
      <% @memories.each do |memory|%>
        <div class="card macard">
          <div class="image-container">
            <%= image_tag "Book.svg", id: "box", class: "card-img"%>
            <p class="calendar-left" type="date"><%=memory.created_at.strftime('%Y-%m-%d')%></p>
            <h3><%= memory.title %></h3>
          </div>
          <div class="card-body">
            <% memory.stamps.each do |stamp|%>
              <p class="card-text">stamp.title</p>
            <% end%>
          </div>
        </div>
      <% end%>
    </div>
  </div>
</div>



